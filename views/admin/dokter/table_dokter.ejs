<div class="table-responsive">
  <table class="table table-striped table-md" id="departemen">
    <thead>                                 
      <tr>
        <th class="text-center">
          No
        </th>
        <th>Nama Dokter</th>
        <th>Departemen</th>
        <th>Spesialis</th>
        <th>Nomor Telepon</th>
        <th>Aksi</th>
      </tr>
    </thead>
    <tbody>                                 
        <% for(let i = 0; i < dokter.length; i++) { %>
            <tr>
                <td><%= i + 1 %></td>
                <td><%= dokter[i].nama %></td>
                <td>
                  <% dokter[i].departement.forEach(departement => { %>
                        <%= departement.name %>
                  <% }) %>
                </td>
                <td><%= dokter[i].spesialis %></td>
                <td><%= dokter[i].no_telepon %></td>
                <td>
                    <form action="/dokter/delete/<%= dokter[i]._id %>?_method=DELETE" method="POST">
                        <a href="/dokter/edit/<%= dokter[i]._id %>" class="btn btn-warning">
                            <i class="fas fa-pencil-alt"></i>
                            
                        </a>
                        <button class="btn btn-danger">
                            <i class="fas fa-trash"></i>
                            
                        </button>
                    </form>
                </td>
            </tr>
        <% } %>
     
    </tbody>
  </table>
</div>